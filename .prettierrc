{
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,

  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^@/"],
  "importOrderSortSpecifiers": true
}
